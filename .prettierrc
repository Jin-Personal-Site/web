{
	"singleQuote": true,
	"trailingComma": "all",
	"semi": false,
	"useTabs": true,
	"tabWidth": 2,
	"jsxSingleQuote": true,
	"tailwindAttributes": ["className"],
	"tailwindFunctions": ["tv", "clsx", "classnames"],
	"tailwindPreserveWhitespace": true,
	"tailwindPreserveDuplicates": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
